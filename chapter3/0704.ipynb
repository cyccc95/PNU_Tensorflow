      "source": [
        "import tensorflow as tf\n",
        "\n",
        "x = tf.Variable(3.0)\n",
        "\n",
        "with tf.GradientTape() as tape2:\n",
        "    with tf.GradientTape() as tape1:\n",
        "        y = x**3\n",
        "    dy = tape1.gradient(y, x)\n",
        "dy2 = tape2.gradient(dy, x)\n",
        "print('dy=', dy.numpy())\n",
        "print('dy2=', dy2.numpy())\n"
      ],
      "outputs": [],
      "execution_count": null
    }
